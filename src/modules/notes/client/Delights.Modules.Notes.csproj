    <PackageReference Include="StardustDL.RazorComponents.Vditors" Version="0.0.2-preview.7" />
    <PackageReference Include="StrawberryShake" Version="11.0.0-preview.137" />
    <PackageReference Include="StardustDL.RazorComponents.MaterialDesignIcons" Version="0.0.2-preview.7" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\..\Modulight.Modules.Client.RazorComponents\Modulight.Modules.Client.RazorComponents.csproj" />
