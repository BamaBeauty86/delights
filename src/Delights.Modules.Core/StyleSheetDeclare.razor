@inject ModuleCollection Collection
@inject IServiceProvider ServiceProvider

@foreach (var c in Collection.Modules)
{
    foreach (var r in c.GetUI(ServiceProvider).Resources)
    {
        if (r.Type is UIResourceType.StyleSheet)
        {
            @((MarkupString)($"<link type=\"text/css\" rel=\"stylesheet\" href=\"{r.Path}\" {Delights.Modules.Core.CoreModuleUI.ResourceTagAttrName}=\"{r.Path}\"/>"))
        }
    }
}